{
  "$schema": "http://json.schemastore.org/package",
  "name": "typescript-doctest",
  "version": "0.1.0",
  "description": "doctest support for typescript",
  "main": "src/main.ts",
  "bin": {
    "typescript-doctest": "dist/src/main.js"
  },
  "scripts": {
    "build": "tsc src/main.ts",
    "test": "ts-node test/*ts src/*doctest.ts | tap-diff",
    "avatest": "tsc && ava dist/test/*js dist/src/*doctest.js",
    "avatest:watch": "tsc --watch & ava --watch dist/test dist/src/*doctest*",
    "coverage": "tsc && nyc ava dist/test",
    "prettier": "prettier --list-different --write src/*ts* test/*ts*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danr/typescript-doctest.git"
  },
  "keywords": [
    "doctest",
    "typescript",
    "testing"
  ],
  "author": "Dan Ros√©n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danr/typescript-doctest/issues"
  },
  "homepage": "https://github.com/danr/typescript-doctest#readme",
  "dependencies": {
    "chokidar": "^2.0.1",
    "minimist": "^1.2.0",
    "typescript": "^2.7.1"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.4",
    "@types/minimist": "^1.2.0",
    "@types/node": "^9.4.5",
    "@types/tape": "^4.2.31",
    "ava": "^0.25.0",
    "faucet": "^0.0.1",
    "prettier": "^1.10.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.8.0"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": true,
    "bracketSpacing": false
  }
}
