{
  "name": "typescript-doctest",
  "version": "0.1.0",
  "description": "doctest support for typescript",
  "main": "src/main.ts",
  "bin": {
    "typescript-doctest": "src/main.js"
  },
  "scripts": {
    "build": "tsc",
    "test:watch": "tsc --watch & ava --watch dist/test",
    "test": "tsc && ava dist/test",
    "coverage": "tsc && nyc ava dist/test",
    "prettier": "prettier --list-different --write src/*ts* test/*ts*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danr/typescript-doctest.git"
  },
  "keywords": [
    "doctest",
    "typescript",
    "testing"
  ],
  "author": "Dan Ros√©n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danr/typescript-doctest/issues"
  },
  "homepage": "https://github.com/danr/typescript-doctest#readme",
  "dependencies": {
    "babel-generator": "^6.26.1",
    "babel-types": "^6.26.0",
    "babylon": "^6.18.0"
  },
  "devDependencies": {
    "@types/babel-generator": "^6.25.1",
    "@types/babel-types": "^7.0.0",
    "@types/babylon": "^6.16.2",
    "@types/node": "^9.4.5",
    "ava": "^0.25.0",
    "prettier": "^1.10.2",
    "typescript": "^2.7.1"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": true,
    "bracketSpacing": false
  }
}
